#include <stdio.h>
int main() {
 int a[] = {14, 7, 9, 21, 7};
 int n = sizeof(a)/sizeof(a[0]);
 int key = 7;
 int found = -1;
 for (int i = 0; i < n; i++) {
 if (a[i] == key) { found = i; break; }
 }
 if (found != -1) printf("Found %d at index %d\n", key, found);
 else printf("Not found\n");
 return 0;
}
